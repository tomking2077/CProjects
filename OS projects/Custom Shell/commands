pwd > pwd.txt
mv pwd.txt newpwd.txt

ls -l /proc/sys | sort -r | head -5
ls -l /proc/sys | sort -r | head -5 | sort > output.txt

ls -l -a > test.txt
grep 19 < "test.txt"  | head -8 | tail -7 | head -4 | sort > "output.txt"

cd /home/ugrads
cd -

sleep 10 &

awk '{print $3}' < test.txt
awk '{print $3}' < test.txt | head -5 | head -3 | sort > awk.txt

grep r < "test.txt" | grep w | head -5 | sort > output.txt

Done
